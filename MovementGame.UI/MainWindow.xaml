<Window x:Class="MovementGame.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MovementGame.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MouseMove="Window_MouseMove" KeyDown="Window_KeyDown">
    <Grid>
        <Label x:Name="labelX" Content="label" HorizontalAlignment="Left" Margin="80,80,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelY" Content="Label" HorizontalAlignment="Left" Margin="140,80,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelZ" Content="Label" HorizontalAlignment="Left" Margin="195,80,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
